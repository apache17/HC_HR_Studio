<%= form_for @employee, url: @url, role: :form, html: { multipart: true } do |f| %>
<%= render partial: "form", locals: {f: f} %>
  
  <div class="input-group-btn" align="center">
    <%= link_to "Cancelar", admin_employees_path, class: "btn bg-red btn-flat" %>
    <%= f.submit "Crear", class: "btn bg-green btn-flat" , id: 'createEmployee'%>
  </div>
<%end%>

<div id = "modal" class="modal" tabindex="-1" role="dialog">
  <div class="modal-dialog" role="document">
    <div class="modal-content">
      <div class="modal-header">
        <h5 class="modal-title">Modal title</h5>
        <button type="button" class="close" data-dismiss="modal" aria-label="Close">
          <span aria-hidden="true">&times;</span>
        </button>
      </div>
      <div class="modal-body">
        <p>Modal body text goes here.</p>
      </div>
      <div class="modal-footer">
        <button type="button" class="btn btn-primary">Save changes</button>
        <button type="button" class="btn btn-secondary" data-dismiss="modal">Close</button>
      </div>
    </div>
  </div>
</div>
